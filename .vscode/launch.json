{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run 3DS Backup Script",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "zsh",
      "args": [
        "${workspaceFolder}/backup_3ds.sh"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Run 3DS Restore Script",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "zsh",
      "args": [
        "${workspaceFolder}/restore_3ds.sh"
      ],
      "console": "integratedTerminal"
    }
  ]
}